<?xml version="1.0"?>

<launch>

  <arg name="env_map" default="$(find neo_mp_500)/configs/navigation/maps/$(env MAP_NAME).yaml" />

  <!-- MAP SERVER -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg env_map)">
  </node>

    <!-- AMCL Node -->
  <node pkg="amcl" type="amcl" name="amcl">

    <remap from="/scan" to="/sick_s300_front/scan_filtered" />

    <rosparam file="$(find neo_mp_500)/configs/navigation/amcl/amcl_basic.yaml" command="load" />

 </node>

</launch>
